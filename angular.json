{
  "$schema": "./node_modules/@angular/cli/lib/config/schema.json",
  "version": 1,
  "newProjectRoot": "projects",
  "projects": {
    "sistema": {
      "projectType": "application",
      "schematics": {},
      "root": "projects/sistema",
      "sourceRoot": "projects/sistema/src",
      "prefix": "amb",
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-angular:browser",
          "options": {
            "outputPath": "dist/sistema",
            "index": "projects/sistema/src/index.html",
            "main": "projects/sistema/src/main.ts",
            "polyfills": "projects/sistema/src/polyfills.ts",
            "tsConfig": "projects/sistema/tsconfig.app.json",
            "assets": [
              "projects/sistema/src/favicon.ico",
              "projects/sistema/src/assets"
            ],
            "styles": [
              "projects/sistema/src/custom-theme.scss",
              "projects/sistema/src/styles.css"
            ],
            "scripts": [
              "./node_modules/@lottiefiles/lottie-player/dist/lottie-player.js"
            ]
          },
          "configurations": {
            "production": {
              "budgets": [
                {
                  "type": "initial",
                  "maximumWarning": "500kb",
                  "maximumError": "5mb"
                },
                {
                  "type": "anyComponentStyle",
                  "maximumWarning": "2kb",
                  "maximumError": "4kb"
                }
              ],
              "fileReplacements": [
                {
                  "replace": "projects/sistema/src/environments/environment.ts",
                  "with": "projects/sistema/src/environments/environment.prod.ts"
                }
              ],
              "outputHashing": "all"
            },
            "development": {
              "buildOptimizer": false,
              "optimization": false,
              "vendorChunk": true,
              "extractLicenses": false,
              "sourceMap": true,
              "namedChunks": true
            }
          },
          "defaultConfiguration": "production"
        },
        "serve": {
          "builder": "@angular-devkit/build-angular:dev-server",
          "configurations": {
            "production": {
              "browserTarget": "sistema:build:production"
            },
            "development": {
              "browserTarget": "sistema:build:development"
            }
          },
          "defaultConfiguration": "development"
        },
        "extract-i18n": {
          "builder": "@angular-devkit/build-angular:extract-i18n",
          "options": {
            "browserTarget": "sistema:build"
          }
        },
        "test": {
          "builder": "@angular-devkit/build-angular:karma",
          "options": {
            "main": "projects/sistema/src/test.ts",
            "polyfills": "projects/sistema/src/polyfills.ts",
            "tsConfig": "projects/sistema/tsconfig.spec.json",
            "karmaConfig": "projects/sistema/karma.conf.js",
            "assets": [
              "projects/sistema/src/favicon.ico",
              "projects/sistema/src/assets"
            ],
            "styles": [
              "projects/sistema/src/styles.css"
            ],
            "scripts": []
          }
        }
      }
    },
    "storybook": {
      "projectType": "application",
      "root": "stories",
      "sourceRoot": "stories",
      "architect": {
        "build": {
          "options": {
            "tsConfig": "tsconfig.json",
            "styles": [],
            "scripts": []
          }
        }
      }
    },
    "dev": {
      "projectType": "library",
      "root": "projects/dev",
      "sourceRoot": "projects/dev/src",
      "prefix": "lib",
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-angular:ng-packagr",
          "options": {
            "project": "projects/dev/ng-package.json"
          },
          "configurations": {
            "production": {
              "tsConfig": "projects/dev/tsconfig.lib.prod.json"
            },
            "development": {
              "tsConfig": "projects/dev/tsconfig.lib.json"
            }
          },
          "defaultConfiguration": "production"
        },
        "test": {
          "builder": "@angular-devkit/build-angular:karma",
          "options": {
            "main": "projects/dev/src/test.ts",
            "tsConfig": "projects/dev/tsconfig.spec.json",
            "karmaConfig": "projects/dev/karma.conf.js"
          }
        }
      }
    }
  },
  "cli": {
    "analytics": false
  },
  "defaultProject": "sistema"
}